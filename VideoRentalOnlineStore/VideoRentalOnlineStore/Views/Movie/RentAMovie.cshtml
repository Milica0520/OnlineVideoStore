@model VideoRentalOnlineStore.Models.ViewModels.RentedMovieVM


<div class="card" style="width: 18rem;">

    <div class="card-body">
        <h5 class="card-title">Movie Title: @Model.MovieTitle</h5>
        <p class="card-text"><strong>Genre:</strong> @Model.RentedOn.ToString("MMMM dd, yyyy")</p>
        <p class="card-text"><strong>Language:</strong> @Model.ReturnedOn.ToString("MMMM dd, yyyy")</p>

    </div>
</div>
<a href="@Url.Action("AllMovies", "Movie")" class="btn btn-primary">Go to Movie list</a>